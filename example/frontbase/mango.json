{
  "version": ">=0.5.0",
  "styles": [
    "styles/screen.styl",
    "styles/test.scss"
  ],
  "scripts": [
    "js/index.js",
    "js/index-next.es6"
  ],
  "images": [
    "img/**/*.{jpg,png,svg}"
  ],
  "sprites": [
    {
      "path": "img/bar/*.svg",
      "name": "bar"
    }
  ],
  "templates": [
    "templates/**/*.jade",
    "!**/_*.jade"
  ],
  "buildstamp": [
    "dist/styles/screen.css",
    "dist/js/index.js"
  ],
  "static": [
    "static/*"
  ],
  "dependencies": [
    "jquery"
  ],
  "watch": [
    "app/**"
  ],
  "data": {
    "external": "data/default.json",
    "any": {
      "other": "objects"
    },
    "or": "values"
  },
  "dist_folder": "dist"
}
